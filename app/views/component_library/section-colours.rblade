      Violet: [{value: "bg-violet-50"}, {value: "bg-violet-100"}, {value: "bg-violet-200"}, {value: "bg-violet-300"}, {value: "bg-violet-400", nesta: 40}, {value: "bg-violet-500", nesta: 70}, {value: "bg-violet-600", nesta: 100, black_aaa: true}, {value: "bg-violet-700", black_aa: true}, {value: "bg-violet-800", white_aa: true}, {value: "bg-violet-900", white_aaa: true}, {value: "bg-violet-950"}],
