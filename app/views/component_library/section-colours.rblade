      Sand: [{value: "bg-sand-50"}, {value: "bg-sand-100"}, {value: "bg-sand-200"}, {value: "bg-sand-300", nesta: 40}, {value: "bg-sand-400", nesta: 70}, {value: "bg-sand-500", nesta: 100}, {value: "bg-sand-600"}, {value: "bg-sand-700", black_aaa: true}, {value: "bg-sand-800", black_aa: true}, {value: "bg-sand-900", white_aa: true}, {value: "bg-sand-950", white_aaa: true}],
    }
  @endruby
  @each (name, colours in colourData)
    <tr>
      <td>{{ name }}</td>
      @each (colour in colours)
        <td class="{{ colour[:value] }}">
          <div class="flex flex-col w-10 h-10 items-center font-medium">
            @if (colour[:nesta])
              <span class="my-auto {{ colours[1][:value] }} rounded leading-none w-min py-px px-0.5">
                {{ colour[:nesta] }}
              </span>
            @end
            @if (colour.keys.intersect? [:black_aaa, :black_aa, :white_aaa, :white_aa])
              <div class="flex gap-0.5 text-xs mt-auto justify-center justify-self-end empty:hidden">
                @if (colour[:black_aaa])
                  <span class="text-black mt-auto">AAA</span>
                @elsif (colour[:black_aa])
                  <span class="text-black mt-auto">AA</span>
                @end
                @if (colour[:white_aaa])
                  <span class="text-white">AAA</span>
                @elsif (colour[:white_aa])
                  <span class="text-white">AA</span>
                @end
              </div>
            @endif
          </div>
        </td>
      @endeach
  @endeach
  </tbody>
</table>